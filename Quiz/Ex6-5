#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main()
{
	// 중첩 반복문
	for (int i = 0; i < 15; i++)
	{
		printf("*");
	}

	printf("\n\n");

	for (int j = 0; j < 3; j++)
	{
		for (int k = 0; k < 5; k++)
		{
			printf("*");
		}
	}

	printf("\n\n");

	for (int j = 0; j < 3; j++)
	{
		for (int k = 0; k < 5; k++)
		{
			printf("*");
		}
		printf("\n");
	}


	return 0;
}
